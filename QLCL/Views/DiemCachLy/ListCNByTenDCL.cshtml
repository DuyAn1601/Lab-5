@{
}
@model List<object>
<table border="1">
    <tr>
        <th>Mã công nhân</th>
        <th>Tên công nhân</th>
        <th>Chức năng</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GetType().GetProperty("MaCongNhan").GetValue(item, null)</td>
            <td>@item.GetType().GetProperty("TenCongNhan").GetValue(item, null)</td>
            <td>
                <a href="/CongNhan/CongNhanDelete">Delete</a>
                <a href="/CongNhan/CongNhanView">View</a>
            </td>
        </tr>
    }
</table>